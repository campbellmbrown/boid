    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,

    "git.branchPrefix": "feature/",

    "gitPrefix.pattern": "\\w+\\/(.*)\\/.*",
    "gitPrefix.replacement": "#$1 ",

    "dotnet.defaultSolution": "Boid.sln",
    "cSpell.words": [
        "boid",
        "boids",
        "tickable"
    ]
}
